<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="fill_parent"
	android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/container" android:orientation="vertical">
	<Spinner android:id="@+id/scaleTypeSpinner"
		android:layout_height="wrap_content" android:layout_width="fill_parent"></Spinner>
	<Button android:id="@+id/spaceButton" android:layout_width="fill_parent" android:layout_height="100dp" android:text="Ingombro"></Button>
	<ImageView android:id="@+id/imageView" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:src="@drawable/marta"></ImageView>
</LinearLayout>
