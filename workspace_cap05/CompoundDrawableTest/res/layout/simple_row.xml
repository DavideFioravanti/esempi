<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="wrap_content" android:layout_width="match_parent">
	<ImageView android:layout_height="wrap_content" android:id="@+id/imageItem"
		android:layout_width="match_parent" android:layout_weight="1"></ImageView>
	<TextView android:layout_height="wrap_content" android:id="@+id/labelItem"
		android:layout_width="match_parent" android:layout_weight="0.2"
		android:text="label"></TextView>
</LinearLayout>
